<?xml version="1.0" encoding="UTF-8"?>

<ros host="ws://192.168.1.136:9090">

	<command component="RobotArm" name="move" >
	
		<dispatch-topic 
			name="/sharework/test/stiima/motion" 
			msg="stiima_listener/MotionTaskExecutionRequestArray"
			publisher="com.github.ros.roxanne_sa.platform.rosbridge.stiima.MotionTaskRequestPublisher" />
			
